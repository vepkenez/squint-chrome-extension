{
    "name": "Squint Beta",
    "manifest_version": 2,
    "version": "1",
    "description": "Digitial Innovation Studio Combating  Misinformation and Creating High Performing Fact-Based Social Content.",
    "browser_action": {
        "name": "Video Context Enhancer",
        "icons": ["icon.png"],
        "default_icon": "icon.png"
    },
    "content_scripts": [
        {
            "js": [ "jquery.min.js", "videodata.js", "content.js" ],
            "css": ["customStyles.css"],
            "matches": ["https://*/*", "http://*/*"]
         }
     ]
}